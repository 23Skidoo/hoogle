:set -fwarn-unused-binds -fwarn-unused-imports
:set -isrc
:load Main

:{
:def opt const $ return $ unlines
        [":!mkdir obj"
        ,":!mkdir obj\\o2"
        ,":!ghc -O2 -isrc --make Main -outputdir obj/o2 -o obj/o2/main -rtsopts"
        ,":!obj\\o2\\main +RTS -T"]
:}
:{
:def prof const $ return $ unlines
        [":!mkdir obj"
        ,":!mkdir obj\\prof"
        ,":!ghc -prof -auto-all -caf-all -isrc --make Main -outputdir obj/prof -o obj/prof/main"  
        ,":!obj\\prof\\main +RTS -p"]
:}
